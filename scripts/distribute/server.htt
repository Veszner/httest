INCLUDE config.htt

SERVER $LISTEN -1
_IGNORE_ERROR ".*"
_RES
#execute transfered scripts and store OK or FAILED into tmp.txt file
_EXEC| $HTTEST_BIN -Ss >/dev/null; if [ $? -eq 0 ]; then echo OK >tmp.txt; else echo FAILED >tmp.txt; fi 
_WAIT
__HTTP/1.1 200 OK
__Host: localhost
__Content-Length: AUTO
__
__----------------------------------------------------
__Result:
_-==
_PIPE
# pipe back OK or FAILED in tmp.txt and delete tmp.txt
_EXEC cat tmp.txt; rm -f tmp.txt

END
