INCLUDE ./config.htt

CLIENT
_EXEC HTT_RAMPUP_DURATION=$HTT_RAMPUP_DURATION HTT_RAMPUP_STEP=$HTT_RAMPUP_INIT_CLIENTS $HTTEST -sT $HTT_RAMPUP_SCRIPT &
_LOOP $HTT_RAMPUP_TO
_EXEC HTT_RAMPUP_DURATION=$HTT_RAMPUP_DURATION HTT_RAMPUP_STEP=$HTT_RAMPUP_STEP $HTTEST -sT $HTT_RAMPUP_SCRIPT &
_SLEEP $HTT_RAMPUP_SLEEP 
_END LOOP
END

SERVER $SUPER_PORT
_RES
_WAIT
__HTTP/1.1 200 OK
__
_CLOSE
_LOOP $HTT_RAMPUP_TO 
_RES
_WAIT
__HTTP/1.1 200 OK
__
_CLOSE
_END LOOP
_EXIT OK
END

