INCLUDE $TOP/test/config.htb

CLIENT
_REQ $YOUR_HOST $YOUR_PORT
__GET /your/path/to/your/resource?your=params HTTP/1.1
__Host: $YOUR_HOST 
__User-Agent: mozilla
__
_EXPECT . "HTTP/1.1 200 OK"
_EXPECT . "BEGIN"
_EXPECT . "AS1"
_EXPECT . "END"
_WAIT
END

SERVER $YOUR_PORT
_RES
_WAIT
__HTTP/1.1 200 OK
__Transfer-Encoding: chunked
_FLUSH
__BEGIN
_CHUNK
__AS1
_CHUNK
__END
_CHUNK
_CHUNK
END
