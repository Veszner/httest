INCLUDE $TOP/test/config.htb

# runs exactly one time
CLIENT

#_SSL:DUMP_START
# also test if we could load cert and key in a client
_REQ $YOUR_HOST SSL:$YOUR_PORT client.cert.pem client.key.pem
__GET /your/path/to/your/resource?your=params HTTP/1.1
__Host: $YOUR_HOST 
__User-Agent: mozilla
__
_EXPECT . "HTTP/1.1 200 OK"
_WAIT
#_SSL:DUMP_STOP info
#_EXPECT Var(info) "Handshake, HelloServer"

_REQ $YOUR_HOST SSL:$YOUR_PORT
__GET /your/path/to/your/resource?your=params HTTP/1.1
__Host: $YOUR_HOST 
__User-Agent: mozilla
__
_EXPECT . "HTTP/1.1 200 OK"
_WAIT

END

SERVER SSL:$YOUR_PORT
_CERT server.cert.pem server.key.pem

_RES
_WAIT
__HTTP/1.1 200 OK
__Content-Length: AUTO 
__Content-Type: text/html
__
__==AS1 - 0==
__

_RES
_WAIT
__HTTP/1.1 200 OK
__Connection: close
__Content-Type: text/html
__
__==AS1 - 0==
__
_CLOSE

END

