INCLUDE $TOP/test/config.htb
INCLUDE $TOP/test/shell.htb

EXEC echo foo bar >myfile

CLIENT
_SLEEP 30000
_EXIT FAILED
END

CLIENT
_SLEEP 2000
_EXPECT Exec "foo bar"
setShellCommandVars
_EXEC $CAT myfile
_SLEEP 1000
_EXEC fail_please 2>$DEV_NULL >$DEV_NULL
END

BLOCK FINALLY
setShellCommandVars
_EXEC $RM myfile
_EXIT OK
END
