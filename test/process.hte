INCLUDE $TOP/test/config.htb

CLIENT
_SLEEP 200
_PROCESS a
_REQ $YOUR_HOST $YOUR_PORT
__POST /foo/1 HTTP/1.1
__Host: $YOUR_HOST 
__
_EXPECT headers "200 OK"
_WAIT
_END PROCESS

_PROCESS b
_REQ $YOUR_HOST $YOUR_PORT
__POST /foo/2 HTTP/1.1
__Host: $YOUR_HOST 
__
_EXPECT headers "404 NOT FOUND"
_WAIT
_END PROCESS

_PROC_WAIT a b 
END

SERVER $YOUR_PORT 2
_RES
_WAIT
__HTTP/1.1 404 NOT FOUND 
__Content-Length: AUTO
__
__NOT FOUND
END

